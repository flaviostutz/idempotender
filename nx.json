{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build-module",
          "test"
        ]
      }
    }
  },
  "targetDefaults": {
    "build-module": {
      "dependsOn": [
        "^build-module"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "outputs": [
        "{projectRoot}/coverage"
      ]
    }
  },
  "defaultBase": "main"
}
